val factorial = factorial(7);

func main() {
    print("Hello, World!");
    print("Factorial: " + factorial);
    print("Squares: " + range(1, 10, :incl).map { i -> return i * i; });
}

func factorial(num) {
    match (num) {
        0, 1: return 1;
        else: return num * factorial(num - 1);
    }
}
